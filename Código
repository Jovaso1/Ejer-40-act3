    private void btnborrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtnumero1.setText("");
        txtnumero2.setText("");
        txtcubo1.setText("");
        txtcubo2.setText("");
        txtraiz1.setText("");
        txtraiz2.setText("");
        txtcuadrado1.setText("");
        txtcuadrado2.setText("");
    }                                         

    private void btnsalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        dispose();
    }                                        

    private void btncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        double numero1, numero2, cubo1, cubo2, raiz1, raiz2, cuadrado1, cuadrado2;
        
        numero1 = Double.parseDouble(txtnumero1.getText());
        numero2 = Double.parseDouble(txtnumero2.getText());
        cubo1 = Operaciones.calcCubo(numero1);
        cubo2 = Operaciones.calcCubo(numero2);
        raiz1 = Operaciones.calcRaiz(numero1);
        raiz2 = Operaciones.calcRaiz(numero2);
        cuadrado1 = Operaciones.calcCuadrado(numero1);
        cuadrado2 = Operaciones.calcCuadrado(numero2);
        
        txtcubo1.setText(String.valueOf(cubo1));
        txtcubo2.setText(String.valueOf(cubo2));
        txtraiz1.setText(String.valueOf(raiz1));
        txtraiz2.setText(String.valueOf(raiz2));
        txtcuadrado1.setText(String.valueOf(cuadrado1));
        txtcuadrado2.setText(String.valueOf(cuadrado2));
    }                                           

