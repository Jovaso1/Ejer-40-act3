Clase:
package ejer.pkg40.act3;
/**
 *
 * @author USER
 */
public class Operaciones {
    public static double calcRaiz(double numero){
    double respuesta1;
    respuesta1 = Math.sqrt(numero);
    return respuesta1;
}
    public static double calcCuadrado(double numero){
    double respuesta2;
    respuesta2 = Math.pow(numero, 2);
    return respuesta2;
}
    public static double calcCubo(double numero){
    double respuesta3;
    respuesta3 = Math.pow(numero, 3);
    return respuesta3;
}
}
